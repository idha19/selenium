<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
  <TestRunParameters>
    <!-- General Config -->
    <Parameter name="AppBaseURL" value="https://automationexercise.com/" />
    <Parameter name="AppName" value="AutomationExercise" />
    <Parameter name="SeleniumBrowser" value="Chrome" />
    <Parameter name="SeleniumBrowserVersion" value="latest" />
